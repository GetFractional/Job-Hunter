      "contract",
      "temporary",
      "permanent",
      "competitive salary",
      "equity",
      "stock options",
      "benefits",
      "health insurance",
      "401k",
      "unlimited pto",
      "vacation",
      "bonus",
      "commission",
      "manager",
      "director",
      "lead",
      "senior",
      "junior",
      "associate",
      "head of",
      "vp of",
      "vice president",
      "specialist",
      "analyst",
      "coordinator",
      "representative"
    ],
    "patterns": [
      {
        "pattern": "^\\d+\\+?\\s*years?\\s*(of)?$",
        "description": "Pure year counts like '5 years', '3+ years'",
        "examples": ["5 years", "3+ years", "10 years of"]
      },
      {
        "pattern": "^\\d+\\-\\d+\\s*years?$",
        "description": "Year ranges like '3-5 years'",
        "examples": ["3-5 years", "5-7 years"]
      },
      {
        "pattern": "^(the|a|an)\\s+.*",
        "description": "Phrases starting with articles (often incomplete extractions)",
        "examples": ["the ability", "a strong", "an excellent"]
      },
      {
        "pattern": "^(and|or|with|for|from)\\s+.*",
        "description": "Phrases starting with conjunctions/prepositions",
        "examples": ["and more", "or similar", "with experience"]
      }
    ]
  },
