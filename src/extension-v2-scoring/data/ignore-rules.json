      "positive attitude",
      "team collaboration",
      "cross-functional collaboration",
      "presentation skills",
      "public speaking",
      "negotiation",
      "negotiation skills",
      "conflict resolution",
      "decision making",
      "decision-making",
      "prioritization",
      "deadline driven",
      "work under pressure",
      "independent",
      "work independently",
      "initiative",
      "ownership",
      "accountability",
      "mentoring",
      "coaching",
      "training",
      "influencing",
      "persuasion",
      "diplomacy",
      "delegation",
      "coordination",
      "empathy",
      "emotional intelligence",
      "active listening",
      "cultural awareness",
      "diversity",
      "inclusion",
      "integrity",
      "honesty",
      "trustworthy",
      "dependable",
      "reliable",
      "punctual",
      "professional",
      "professionalism",
      "customer service",
      "service oriented",
      "service-oriented"
    ],
    "patterns": [
      {
        "pattern": ".*\\s+skills?$",
        "description": "Generic 'X skills' phrases",
        "examples": ["soft skills", "people skills", "communication skills"]
      },
      {
        "pattern": "^strong\\s+.*",
        "description": "Phrases starting with 'strong'",
        "examples": ["strong communication", "strong leadership"]
      },
      {
        "pattern": "^excellent\\s+.*",
        "description": "Phrases starting with 'excellent'",
        "examples": ["excellent communication", "excellent interpersonal"]
      },
      {
        "pattern": "^good\\s+.*",
        "description": "Phrases starting with 'good'",
        "examples": ["good communication", "good teamwork"]
      },
      {
        "pattern": "^great\\s+.*",
        "description": "Phrases starting with 'great'",
        "examples": ["great communication", "great team player"]
      },
      {
        "pattern": ".*ability$",
        "description": "Phrases ending with 'ability'",
        "examples": ["communication ability", "leadership ability"]
      },
      {
        "pattern": "^ability\\s+to\\s+.*",
        "description": "Phrases starting with 'ability to'",
        "examples": ["ability to communicate", "ability to lead"]
      },
      {
        "pattern": ".*oriented$",
        "description": "Phrases ending with 'oriented'",
        "examples": ["detail oriented", "results oriented"]
      },
      {
        "pattern": ".*thinking$",
        "description": "Phrases ending with 'thinking' (too vague)",
        "examples": ["analytical thinking", "strategic thinking"]
      },
      {
        "pattern": ".*mindset$",
        "description": "Phrases ending with 'mindset'",
        "examples": ["growth mindset", "entrepreneurial mindset"]
      }
    ]
  },
  "junkPhrases": {
    "exactMatches": [
      "etc",
      "etc.",
      "more",
      "and more",
      "blah",
      "other",
      "others",
      "various",
      "multiple",
      "including",
      "such as",
      "e.g.",
      "i.e.",
      "related",
      "relevant",
      "similar",
      "experience",
      "years of experience",
      "years experience",
      "minimum years",
      "required",
      "preferred",
      "desired",
      "nice to have",
      "bonus",
      "plus",
      "must have",
      "bachelor's degree",
      "bachelor degree",
      "master's degree",
      "masters degree",
      "mba",
      "degree required",
      "equivalent experience",
      "related field",
      "fast paced",
